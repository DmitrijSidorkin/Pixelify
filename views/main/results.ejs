<% layout('layouts/boilerplate')%>
<input
  id="results-data"
  type="hidden"
  name="results-data"
  value="<%=playSessionData%>"
/>
<input
  id="highscores-data"
  type="hidden"
  name="highscores-data"
  value="<%=highscoresData%>"
/>
<input
  id="default-profile-img"
  type="hidden"
  name="default-profile-img"
  value="<%=defaultProfileImg%>"
/>
<div class="results-box">
  <img src="" alt="results" id="results-image" />
  <div class="results-text">
    <h1 id="results-title"></h1>
    <div id="results-details"></div>
    <div id="all-answers"></div>
  </div>
</div>
<button onclick="detailedResults()">Detailed Results</button>

<div class="highscores-box">
  <div class="highscores-title"><%-highscoreText%></div>
  <div id="highscores"></div>
</div>
<script src="/javascripts/displayResults.js"></script>
